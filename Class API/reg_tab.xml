<?xml version="1.0" encoding="utf-8"?>
<snippet filetemplate="false" extension="">
<name>reg_tab</name>
<help>Register a tab using reg_tab.cfm</help>
<starttext><![CDATA[<cfmodule 	template="/#request.SEMap#/config/init/reg_tab.cfm"
			module="$${module:#attributes.AppPath#/tabs/module_here.cfm}"
			priority="$${priority:0}"
			class="$${class:APPINSTANCE|APP}"
			initiallyselected="$${initiallyselected:no|yes}"
			perms="$${perms:READ|WRITE|ADMIN}"
>]]></starttext>
<endtext><![CDATA[]]></endtext>
</snippet>