<?xml version="1.0" encoding="utf-8"?>
<snippet filetemplate="false" extension="">
<name>seform</name>
<help>SEForm tag</help>
<starttext><![CDATA[<seform:form
	id="$${id}"
	action="$${action:#attributes.selfURL#}"
